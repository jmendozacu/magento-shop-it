{
  "name": "popov/shop-it",
  "license": "BSD-3-Clause",
  "type": "magento-module",
  "description": "Shop IT",
  "authors":[{
    "name": "Serhii Popov"
  }],
  "require": {
    "magento-hackathon/magento-composer-installer": "*",
    "aydin-hassan/magento-core-composer-installer" : "~1.0",
    "magento/core": "1.9.3.6",
    "dweeves/magmi": "dev-master",
    "popov/magento-deleter": "dev-master",
    "popov/magento-magmi-import": "dev-master",
    "agere/magento-mirasvit-advanced-product-feeds": "dev-master",
    "mage-eag/mage-enhanced-admin-grids": "0.9.*",
    "rossmc/rsqs": "dev-master"
  },
  "require-dev": {
    "ecomdev/mage-ci": "dev-master"
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://packages.firegento.com"
    },
    {
      "type": "vcs",
      "url": "https://github.com/agereteam/magento-mirasvit-advanced-product-feeds.git"
