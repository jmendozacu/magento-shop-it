    },
    {
      "type": "git",
      "url": "https://github.com/popovserhii/magento-magmi-import.git"
    },
    {
      "type": "git",
      "url": "https://github.com/popovserhii/RemoveStoreQueryString.git"
    },
    {
      "type": "git",
      "url": "https://github.com/mage-eag/mage-enhanced-admin-grids.git"
    }
  ],
  "extra": {
    "magento-root-dir": "htdocs/",
    "magento-deploystrategy": "symlink",
    "auto-append-gitignore": true,

    "magento-force": true
  }
